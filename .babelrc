{
  "presets": ["env", "react"],
  "plugins": [
    "transform-class-properties",
    "transform-object-rest-spread",
    [
      "import",
      {
        "libraryName": "antd",
        "style": true
      }
      // "import",
      // { "libraryName": "ant-design-pro",  "style": true }
    ],
    [
      "module-resolver",
      {
        "root": ["./"],
        "alias": {
          "dist": "./dist",
          "components": "./src/components",
          "views": "./src/views"
        }
      }
    ]
  ]
}
